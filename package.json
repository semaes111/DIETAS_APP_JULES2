{
  "name": "nutrimed-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "node scripts/seed-production.js",
    "db:setup-supabase": "node scripts/setup-supabase.js",
    "db:rls-policies": "echo 'Run the SQL in scripts/setup-rls-policies.sql in your Supabase SQL Editor'",
    "deploy:supabase": "node scripts/deploy-supabase.js",
    "deploy:prepare": "npm run type-check && npm run lint && npm run test:ci",
    "backup": "node scripts/backup-supabase.js backup",
    "backup:monitor": "node scripts/backup-supabase.js monitor",
    "backup:full": "node scripts/backup-supabase.js full",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && next build",
    "type-check": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next-auth/prisma-adapter": "^1.0.7",
